{
  "name": "Tools for Paper",
  "version": "1.4.2",
  "manifest_version": 2,
  "description": "Read more easily and quickly",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "nr.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["inject.js"]
    },
    {
      "matches": [
        "http://scholar.google.com/*",
        "http://scholar.google.co.jp/*",
        "https://scholar.google.com/*",
        "https://scholar.google.co.jp/*"
      ],
      "js": [
        "tip.js",
        "scholar-inject.js"
      ],
      "css": [
        "tip.css"
      ]
    }
  ],
  "icons": {
    "16": "icon16.ico"
  }
}
